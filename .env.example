# Kit IA Emprendedor Extension - Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration (Required)
# Get these from: https://app.supabase.com/project/nktqqsbebhoedgookfzu/settings/api
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Development Settings (Optional)
VITE_DEV_MODE=false
VITE_MOCK_DATA=false
VITE_BYPASS_AUTH=false

# URLs (Optional - defaults in config.js)
VITE_WEBSITE_URL=https://kitiaemprendedor.com
VITE_SUPPORT_URL=https://kitiaemprendedor.com/support
